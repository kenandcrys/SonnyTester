{
  "name": "sonnyny-js-fullstack",
  "version": "3.0.0",
  "description": "Full Stack SonnyNY",
  "main": "index.js",
  "scripts": {
    "install": "npm --prefix Backend install && npm --prefix Frontend install",
    "dev": "concurrently \"npm run backend\" \"sleep 5 && npm run frontend\"",
    "backend": "npm --prefix Backend run dev",
    "frontend": "npm --prefix Frontend run dev",
    "resetdb": "npm --prefix Backend run resetdb",
    "migrate": "npm --prefix Backend run migrate",
    "seed": "npm --prefix Backend run seed"
  },
  "author": "Fish, Jess W., and Michael W.",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.3.0"
  },
  "dependencies": {
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7-rc.0"
  }
}
